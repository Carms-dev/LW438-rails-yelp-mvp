<%= simple_form_for restaurant, html: { id: "new_restaurant" } do |f| %>
    <%= f.input :name, html: { id: "restaurant_name" } %>
    <%= f.input :address, html: { id: "restaurant_address" } %>
    <%= f.input :phone_number, html: { id: "restaurant_phone_number" } %>
    <%= f.input :category, html: { id: "restaurant_category" } %>
    <%= f.submit %>
    <ul id="error_display"></ul>
<% end %>